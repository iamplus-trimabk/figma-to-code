{
  "providers": {
    "google": {
      "name": "Google Analytics",
      "description": "Google Analytics 4 integration",
      "install_command": "npm install gtag-js",
      "config_files": ["src/lib/analytics/google.ts"],
      "env_vars": ["VITE_GA_MEASUREMENT_ID"]
    },
    "plausible": {
      "name": "Plausible Analytics",
      "description": "Privacy-focused analytics",
      "install_command": "npm install plausible-tracker",
      "config_files": ["src/lib/analytics/plausible.ts"],
      "env_vars": ["VITE_PLAUSIBLE_DOMAIN"]
    },
    "custom": {
      "name": "Custom Analytics",
      "description": "Custom analytics implementation",
      "install_command": "",
      "config_files": ["src/lib/analytics/custom.ts"],
      "env_vars": []
    }
  },
  "events": [
    "page_view",
    "user_interaction",
    "form_submission",
    "error_occurred"
  ],
  "tracking_config": {
    "enable_auto_tracking": true,
    "enable_consent_management": true,
    "enable_debug_mode": false
  }
}